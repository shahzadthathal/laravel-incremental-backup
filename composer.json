{
    "name": "shahzadthathal/laravel-incremental-backup",
    "description": "A Laravel package for incremental backups supporting S3, Azure, and local disk with MySQL backup support.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Muhammad Shahzad",
            "email": "shahzadthathal@gmail.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "illuminate/support": "^10.0",
        "league/flysystem": "^3.0",
        "aws/aws-sdk-php": "^3.0",
        "microsoft/azure-storage-blob": "^1.5",
        "symfony/process": "^6.0"
    },
    "autoload": {
        "psr-4": {
            "ShahzadThathal\\IncrementalBackup\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "ShahzadThathal\\IncrementalBackup\\BackupServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
